{
    "rules" : [
        {
            "name": "Date Format",
            "ruleID": "date",
            "ruleDescription": "Identifies incorrectly formatted date entries",
            "option": true,
            "optionType": "checklist",
            "ruleOptions": [
                {
                    "displayName": "mm/dd/yyyy (11/27/1995)",
                    "pixelName": "mm/dd/yyyy"
                },
                {
                    "displayName": "month dd, yyyy (November 27, 1995)",
                    "pixelName": "month dd, yyyy"
                },
                {
                    "displayName": "day, month dd, yyyy (Monday, November 27, 1995)",
                    "pixelName": "day, month dd, yyyy"
                },
                {
                    "displayName": "mon dd, yyyy (Nov 27, 1995)",
                    "pixelName": "mon dd, yyyy"
                }
            ]
        },
        {
            "name": "Email Format",
            "ruleID": "email",
            "ruleDescription": "Identifies incorrectly formatted email entries",
            "option": true,
            "optionType": "checklist",
            "ruleOptions": [
                {
                    "displayName": "Valid emails with one (1) .xxx in the address",
                    "pixelName": "xxxxx@xxxx.xxx"
                },
                {
                    "displayName": "Valid emails with more than one .xxx in the address",
                    "pixelName": "xxxxx@xxxx.xx.xx"
                },
                {
                    "displayName": "Any valid email address",
                    "pixelName": "xxxxx@xxxx.xxx(.xx)"
                }
            ]
        },
        {
            "name": "Name Format",
            "ruleID": "name",
            "ruleDescription": "Identifies incorrectly formatted name entries",
            "option": true,
            "optionType": "checklist",
            "ruleOptions": [
                {
                    "displayName": "last, first (optional middle initial)",
                    "pixelName": "last, first (m.)"
                },
                {
                    "displayName": "First Last",
                    "pixelName": "first last"
                }
            ]
        },  
        {
            "name": "Blanks/Nulls/NAs",
            "ruleID": "blanks",
            "ruleDescription": "Identifies blank cells",
            "option": false,
            "optionType": false,
            "ruleOptions": false
        },  
        {
            "name": "Duplicates",
            "ruleID": "duplicates",
            "ruleDescription": "Identifies all non-unique values",
            "option": false,
            "optionType": "n/a",
            "ruleOptions": [
                {
                    "displayName": "n/a",
                    "pixelName": ""
                }
            ]
        },  
        {
            "name": "Validate Values",
            "ruleID": "validate",
            "ruleDescription": "Validates the data against an input list",
            "option": true,
            "optionType": "input",
            "ruleOptions": false
        }
    ]
}