<assembly xmlns="http://maven.apache.org/ASSEMBLY/2.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.0.0 http://maven.apache.org/xsd/assembly-2.0.0.xsd">
  <id>semosshome</id>
  <formats>
    <format>tar.gz</format>
  </formats>
  <fileSets>
    <fileSet>
      <directory>config</directory>
      <outputDirectory>config</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>db</directory>
      <outputDirectory>db</outputDirectory>
      <includes>
        <include>Default**/*</include>
        <include>form_builder_engine**/*</include>
        <include>form_builder_engine.smss</include>
        <include>LocalMasterDatabase**/*</include>
        <include>LocalMasterDatabase.smss</include>
        <include>security**/*</include>
        <include>security.smss</include>
		<include>themes**/*</include>
		<include>themes.smss</include>
        <include>foaf.xml</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>export</directory>
      <outputDirectory>export</outputDirectory>
      <includes>
        <include>ZIPs/</include>
      </includes>
    </fileSet>
    <fileSet>
    	<directory>py</directory>
        <outputDirectory>py</outputDirectory>
    	<includes>
    	  <include>analytics.py</include>
    	  <include>clean.py</include>
    	</includes>
    </fileSet>
    <fileSet>
      <directory>images</directory>
      <outputDirectory>images</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>InsightCache</directory>
      <outputDirectory>InsightCache</outputDirectory>
      <includes>
        <include>CSV_Insights/</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>NLPartifacts</directory>
      <outputDirectory>NLPartifacts</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>R</directory>
      <outputDirectory>R</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>rdbms</directory>
      <outputDirectory>rdbms</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>RDFGraphLib</directory>
      <outputDirectory>RDFGraphLib</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>rpa</directory>
      <outputDirectory>rpa</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>upload</directory>
      <outputDirectory>upload</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>WordNet-3.1</directory>
      <outputDirectory>WordNet-3.1</outputDirectory>
    </fileSet>
  </fileSets>
  <files>
    <file>
      <source>license.txt</source>
    </file>
    <file>
      <source>log4j.prop</source>
    </file>
    <file>
      <source>RDF_Map.prop</source>
    </file>
    <file>
      <source>social.properties</source>
    </file>
	<file>
      <source>ver.txt</source>
    </file>
	<file>
      <source>AuditModificationView.json</source>
    </file>
	<file>
      <source>AuditTimelineView.json</source>
    </file>
	<file>
      <source>ExploreInstanceDefaultWidget.json</source>
    </file>
    <file>
      <source>reactorToWidget.csv</source>
    </file>
  </files>
</assembly>